{
  "version": 2,
  "projects": {
    "blog": {
      "projectType": "application",
      "root": "apps/blog",
      "sourceRoot": "apps/blog/src",
      "prefix": "ngfizzy",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/blog",
            "index": "apps/blog/src/index.html",
            "main": "apps/blog/src/main.ts",
            "polyfills": "apps/blog/src/polyfills.ts",
            "tsConfig": "apps/blog/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/blog/src/favicon.ico",
              "apps/blog/src/assets"
            ],
            "styles": [
              "apps/blog/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/ngx-markdown-editor/dist/assets/highlight.js/agate.min.css",
              "node_modules/ngx-markdown-editor/dist/assets/highlight.js/agate.min.css",
              "node_modules/prismjs/themes/prism-okaidia.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "node_modules/ngx-markdown-editor/dist/assets/highlight.js/highlight.min.js",
              "node_modules/ngx-markdown-editor/dist/assets/marked.min.js",
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-csharp.min.js",
              "node_modules/prismjs/components/prism-css.min.js",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/blog/src/environments/environment.ts",
                  "with": "apps/blog/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "blog:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "blog:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "blog:build"
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/blog/tsconfig.app.json",
              "apps/blog/tsconfig.spec.json",
              "apps/blog/tsconfig.editor.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/blog/**/*"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/blog"
          ],
          "options": {
            "jestConfig": "apps/blog/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "blog-e2e": {
      "root": "apps/blog-e2e",
      "sourceRoot": "apps/blog-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-e2e/cypress.json",
            "tsConfig": "apps/blog-e2e/tsconfig.e2e.json",
            "devServerTarget": "blog:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/blog-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/blog-e2e/**/*"
            ]
          }
        }
      }
    },
    "bff": {
      "root": "apps/bff",
      "sourceRoot": "apps/bff/src",
      "projectType": "application",
      "prefix": "bff",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/bff",
            "main": "apps/bff/src/main.ts",
            "tsConfig": "apps/bff/tsconfig.app.json",
            "assets": [
              "apps/bff/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/bff/src/environments/environment.ts",
                  "with": "apps/bff/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "bff:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bff/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/bff"
          ],
          "options": {
            "jestConfig": "apps/bff/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "blog"
}